// 文档教程: https://uniapp.dcloud.net.cn/uniCloud/schema
{
	"bsonType": "object",
	"required": ["label"],
	"permission": {
		"read": false,
		"create": false,
		"update": true,
		"delete": true
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"create_time": {
			"bsonType": "timestamp",
			"description": "创建时间",
			"label": "创建时间"
		},
		"update_time": {
			"bsonType": "timestamp",
			"description": "更新时间",
			"label": "更新时间"
		},
		"coop_name": {
			"bsonType": "string",
			"description": "请输入合作社名称",
			"label": "合作社名称",
			"trim": "both"
		},
		"contact_name": {
			"bsonType": "string",
			"description": "请输入联系人姓名",
			"label": "联系人姓名",
			"trim": "both"
		},
		"contact_phone": {
			"bsonType": "string",
			"description": "请输入联系人电话",
			"label": "联系人电话",
			"pattern": "^1[34578][0-9]{9}$",
			"trim": "both"
		},
		"image": {
			"bsonType": "file",
			"fileMediaType": "image",
			"fileExtName": "jpg,png,webp",
			"description": "请选择合作社图片",
			"label": "合作社图片"
		},
		"disabled": {
			"bsonType": "bool",
			"description": "禁用后，用户端不会显示",
			"label": "是否禁用",
			"defaultValue": "false"
		}
	}
}