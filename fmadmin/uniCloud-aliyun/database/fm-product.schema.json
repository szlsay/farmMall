// 文档教程: https://uniapp.dcloud.net.cn/uniCloud/schema
{
	"bsonType": "object",
	"required": [],
	"permission": {
		"read": true,
		"create": true,
		"update": true,
		"delete": true
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"is_delete": {
			"bsonType": "bool",
			"label": "是否删除",
			"defaultValue": false
		},
		"create_uid": {
			"bsonType": "string",
			"description": "创建人用户的唯一id",
			"label": "创建人用户id",
			"forceDefaultValue": {
				"$env": "uid"
			},
			"foreignKey": "uni-id-users._id"
		},
		"create_time": {
			"bsonType": "timestamp",
			"description": "创建时间",
			"title": "创建时间",
			"defaultValue": {
				"$env": "now"
			}
		},
		"update_time": {
			"bsonType": "timestamp",
			"description": "更新时间",
			"title": "更新时间"
		},
		"name": {
			"bsonType": "string",
			"description": "请填写产品名称",
			"label": "产品名称",
			"maxLength": 10,
			"trim": "both"
		},
		"raw_name": {
			"bsonType": "string",
			"description": "请填写原材料名称",
			"label": "原材料名称",
			"maxLength": 10,
			"trim": "both"
		},
		"unit": {
			"bsonType": "string",
			"description": "请填写计量单位",
			"label": "计量单位",
			"componentForEdit": {
				"name": "uni-data-select",
				"props": {
					"placeholder": "请选择计量单位"
				}
			}
		},
		"unit_title": {
			"bsonType": "string",
			"description": "请填写计量单位",
			"label": "计量单位"
		},
		"image": {
			"bsonType": "file",
			"fileMediaType": "image",
			"fileExtName": "jpg,png,webp",
			"description": "请选择产品主图",
			"label": "产品主图"
		},
		"image_content": {
			"bsonType": "array",
			"arrayType": "file",
			"fileMediaType": "image",
			"fileExtName": "jpg,png,webp",
			"description": "请选择展示图片",
			"label": "展示图片"
		},
		"raw_cost": {
			"bsonType": "double",
			"description": "请填写原材料成本",
			"label": "原材料成本",
			"defaultValue"
			0,
			"minimum": 0,
			"maximum": 1000000,
			"errorMessage": "{label}在{minimum}与{maximum}之间"
		},
		"yield_cost": {
			"bsonType": "double",
			"description": "请填写出成率",
			"label": "出成率",
			"defaultValue"
			0,
			"minimum": 0,
			"maximum": 1,
			"errorMessage": "{label}在{minimum}与{maximum}之间"
		},
		"processing_cost": {
			"bsonType": "double",
			"description": "请填写加工成本",
			"label": "加工成本",
			"defaultValue": 0,
			"minimum": 0,
			"maximum": 1000000,
			"errorMessage": "{label}在{minimum}与{maximum}之间"
		},
		"finish_cost": {
			"bsonType": "double",
			"description": "请填写成品成本",
			"label": "成品成本",
			"defaultValue": 0,
			"minimum": 0,
			"maximum": 1000000,
			"errorMessage": "{label}在{minimum}与{maximum}之间"
		},
		"transport_cose": {
			"bsonType": "double",
			"description": "请填写运储成本",
			"label": "运储成本",
			"defaultValue": 0,
			"minimum": 0,
			"maximum": 1000000,
			"errorMessage": "{label}在{minimum}与{maximum}之间"
		},
		"reproduct_cose": {
			"bsonType": "double",
			"description": "请填写再生产成本",
			"label": "再生产成本",
			"defaultValue": 0,
			"minimum": 0,
			"maximum": 1000000,
			"errorMessage": "{label}在{minimum}与{maximum}之间"
		},
		"sideline_income": {
			"bsonType": "double",
			"description": "请填写副产品收入",
			"label": "副产品收入",
			"defaultValue": 0,
			"minimum": 0,
			"maximum": 1000000,
			"errorMessage": "{label}在{minimum}与{maximum}之间"
		},
		"sideline_income": {
			"bsonType": "double",
			"description": "请填写副产品收入",
			"label": "副产品收入",
			"defaultValue": 0,
			"minimum": 0,
			"maximum": 1000000,
			"errorMessage": "{label}在{minimum}与{maximum}之间"
		},
		"quality_fund": {
			"bsonType": "double",
			"description": "请填写质信金",
			"label": "质信金",
			"defaultValue": 0,
			"minimum": 0,
			"maximum": 1,
			"errorMessage": "{label}在{minimum}与{maximum}之间"
		},
		"sum_cose": {
			"bsonType": "double",
			"description": "请填写成本小计",
			"label": "成本小计",
			"defaultValue": 0,
			"minimum": 0,
			"maximum": 1000000,
			"errorMessage": "{label}在{minimum}与{maximum}之间"
		}
	}
}